<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App_Training_Xamarin.Views.FlyoutView.FlyoutPageMain"
             xmlns:view="clr-namespace:App_Training_Xamarin"
            xmlns:pages="clr-namespace:App_Training_Xamarin.Views.FlyoutView">
  <FlyoutPage.Flyout>
    <pages:FlyoutPageMainFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <view:MainPage />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>