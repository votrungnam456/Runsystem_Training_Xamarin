<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App_Training_Xamarin.Views.TabbedView.TabbedChild2">
    <NavigationPage.TitleView>
        <Label Text="This is navigation page in the tabbed page"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
    </NavigationPage.TitleView>
    <ContentPage.Content>
        <StackLayout>
            <Label Text="This is tabbed child page 2"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Navigation in Tabbed child" Clicked="Navigation_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>